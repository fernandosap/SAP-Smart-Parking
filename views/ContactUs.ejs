<section id="contact" class="parallax-section">
     <div class="container">
          <div class="row">

               <div class="col-md-12 col-sm-12">
                    <!-- SECTION TITLE -->
                    <div class="wow fadeInUp section-title" data-wow-delay="0.2s">
                         <h2>Contáctanos</h2>
                         <p>Cualquier duda o comentario avísanos. Nos pondremos en contacto contigo.</p>
                    </div>
               </div>

               <div class="col-md-7 col-sm-10">
                    <!-- CONTACT FORM HERE -->
                    <div class="wow fadeInUp" data-wow-delay="0.4s">
                        <form id="contact-form" action="#" method="post">
                              <div class="col-md-6 col-sm-6">
                                   <input type="text" class="form-control" name="name" placeholder="Nombre" required="">
                              </div>
                              <br>
                              <div class="col-md-6 col-sm-6">
                                   <input type="email" class="form-control" name="email" placeholder="Email" required="">
                              </div>
                              <br>
                              <div class="col-md-12 col-sm-12">
                                   <textarea class="form-control" rows="5" name="message" placeholder="Mensaje" required=""></textarea>
                              </div>
                              <br>
                              <div class="col-md-offset-8 col-md-4 col-sm-offset-6 col-sm-6">
                                   <button id="submit" type="submit" class="btn btn-lg btn-primary" name="submit">Enviar Mensaje</button>
                              </div>
                        </form>
                    </div>
               </div>


               <div class="col-md-5 col-sm-8">
                    <!-- CONTACT INFO -->
                    <div class="wow fadeInUp contact-info" data-wow-delay="0.4s">
                         <div class="section-title">
                          <br>
                              <h2>Información de contacto</h2>
                              <p></p>
                         </div>
                         
                         <p><i class="fa fa-map-marker"></i>Prolongación Paseo de la Reforma #600</p>
                         <p><i class="fa fa-comment"></i> <a href="mailto:info@company.com">support@bestrunparking.com</a></p>
                         <p><i class="fa fa-phone"></i> 010-020-0340</p>
                    </div>
               </div>

          </div>
     </div>
</section>

