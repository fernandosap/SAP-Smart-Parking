<!-- modal para reservar el espacio -->

<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="tituloModal">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="text-center"> 
          <p><b>Pick the date and time you would like to reserve </b></p>
          <p>Start date: </p>
          <p><input type="date" name="fecha_inicio" id="fecha_inicio"/></p>
          <p>Start time: </p>
          <p><input type="time" name="hora_inicio" id="hora_inicio"/></p>
          <p>End date: </p>
          <p><input type="date" name="fecha_fin" id="fecha_fin"/></p>
          <p>End time: </p>
          <p><input type="time" name="hora_fin" id="hora_fin"/></p>
          <p>
          <div class="dropdown">

            <button class="btn btn-success dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              Select your car.
            </button>

            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton" id="dropdownVehiculos">
            </div>
          </div>
        </p>

        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" id="botonConfirmarReserva" onclick="confirmarReserva(); return false" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

<!-- modal de confirmaciÃ³n de reserva -->

<div class="modal fade" id="modalConfirmacion" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="tituloModal">Congratulations!</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="text-center" id="MensajeModalConfirmacion"> 
          <p><b>Your resevation has been succesfully saved. </b></p>
        </div>
        <div>
        <p class="text-center" id="direccion_gmaps_boton"><a class="btn btn-success text-center" href="#" target="_blank" id="direccion_gmaps">Take me there!</a></p>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

